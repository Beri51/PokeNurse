{
  "name": "PokeNurse",
  "version": "1.5.4",
  "description": "A tool for Pokémon Go to aid in transferring and evolving Pokémon",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production electron ./app/",
    "buildstats": "node ./generateBaseStats",
    "lint": "eslint --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty app main *.js",
    "hot-dashboard": "yarn run hot-server -- --dashboard",
    "hot-server": "node --max_old_space_size=2096 -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "yarn run build-main && yarn run build-renderer",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development.js",
    "package": "npm run build && build --publish never",
    "package-win": "npm run build && build --win --x64",
    "package-linux": "npm run build && build --linux",
    "package-all": "npm run build && build -mwl",
    "postinstall": "concurrently \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "dev": "yarn run hot-server"
  },
  "build": {
    "appId": "com.vinnymac.PokeNurse",
    "category": "public.app-category.tools",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.js",
      "main.js.map",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    }
  },
  "directories": {
    "buildResources": "resources",
    "output": "release"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinnymac/PokeNurse.git"
  },
  "keywords": [
    "pokemon",
    "go"
  ],
  "author": "duhminick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vinnymac/PokeNurse/issues"
  },
  "homepage": "https://github.com/vinnymac/PokeNurse#readme",
  "devDependencies": {
    "asar": "0.12.3",
    "async-file": "2.0.1",
    "babel-core": "6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-webpack-loaders": "0.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "6.14.0",
    "concurrently": "3.0.0",
    "cross-env": "3.0.0",
    "css-loader": "0.25.0",
    "del": "^2.2.2",
    "devtron": "^1.3.0",
    "electron-builder": "^8.6.0",
    "electron-debug": "^1.0.1",
    "electron-devtools-installer": "^2.0.1",
    "electron-localshortcut": "0.6.1",
    "electron-prebuilt": "1.4.1",
    "eslint": "3.4.0",
    "eslint-config-airbnb": "10.0.1",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "0.6.0",
    "eslint-plugin-import": "1.15.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-react": "6.3.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.7.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "jquery": "^2.2.4",
    "json-loader": "^0.5.4",
    "lodash": "4.16.2",
    "minimist": "^1.2.0",
    "moment": "2.15.1",
    "postcss": "^5.2.6",
    "react": "15.3.2",
    "react-bootstrap": "^0.30.3",
    "react-burger-menu": "^1.10.4",
    "react-dom": "15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-actions": "0.12.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "semver": "^5.3.0",
    "style-loader": "^0.13.1",
    "webpack": "1.13.2",
    "webpack-dashboard": "0.2.0",
    "webpack-dev-middleware": "1.8.3",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^0.14.1",
    "webpack-validator": "^2.2.9"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "font-awesome": "4.6.3",
    "node-pogo-protos": "1.5.0",
    "pogobuf": "1.7.0",
    "source-map-support": "^0.4.2"
  },
  "devEngines": {
    "node": ">=4.x",
    "npm": ">=3.x"
  }
}
